{
    "routes": [
        {
            "src": "/assets/(.*)\\.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/assets/$1.js"
        },
        {
            "src": "/assets/(.*)\\.css",
            "headers": {
                "Content-Type": "text/css"
            },
            "dest": "/assets/$1.css"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}